
<div class='dev_head'>
    <img class='head_logo' src='/images/ui/logo.jpg'>
</div>
<!-- 导航条 -->
<div class='dev-nav'>
    <div class='dev-nav-list'>
        <%if(select == "index"){%>
        <div class='dev-nav-item' style="background-color: #4ba4be">
        <%}else {%>
        <div class='dev-nav-item' style="background-color: #4ba4be">
        <%}%>
            <a class='dev-nav-title'  href="./">
                <div>首页</div>
            </a>
        </div>
        <div class='dev-nav-split'>
            <div class='dev-nav-split-content'></div>
        </div>
        <div class='dev-nav-item' style="background-color: #4ba4be">
            <a class='dev-nav-title'>
                <div>产品</div>
            </a>
            <ul class='dev-nav-more'>
                <%for(var i=0; i < base_info.typesArr.length; i++){
                    if(base_info.typesArr[i].name){%>
                    <a href="./product?index=<%=base_info.typesArr[i].type%>&page=1">
                        <li>
                            <%=base_info.typesArr[i].name%>
                        </li>
                    </a>
                <%}}%>
            </ul>
        </div>
        <div class='dev-nav-split'>
            <div class='dev-nav-split-content'></div>
        </div>
        <%if(select == "hzhb"){%>
        <div class='dev-nav-item' style="background-color: #4ba4be">
        <%}else {%>
        <div class='dev-nav-item' style="background-color: #4ba4be">
        <%}%>
            <a class='dev-nav-title' href="./hzhb">
                <div>合作伙伴</div>
            </a>
        </div>
        <div class='dev-nav-split'>
            <div class='dev-nav-split-content'></div>
        </div>
        <%if(select == "about"){%>
        <div class='dev-nav-item' style="background-color: #4ba4be">
        <%}else {%>
        <div class='dev-nav-item' style="background-color: #4ba4be">
        <%}%>
            <a class='dev-nav-title' href="./about">
                <div>联系我们</div>
            </a>
        </div>
    </div>
</div>
<div class="swiper-container" id="slider1">
    <div class="swiper-wrapper">
        <div class="swiper-slide"> <a><img src="/images/slider/slider_1.jpg" class="img-responsive"></a> </div>
        <!-- <div class="swiper-slide"> <a><img src="/images/slider/slider_2.jpg" class="img-responsive"></a> </div> -->
        <!-- <div class="swiper-slide"> <a><img src="/images/slider/slider_3.jpg" class="img-responsive"></a> </div> -->
    </div>
    <!-- 如果需要分页器 -->
    <div class="swiper-pagination"></div>
</div>

