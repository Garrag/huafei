<script src="/javascripts/JxSwiper.js"></script>

<script>
    $(function () {
        var swiper = new Swiper('.swiper-container', {
            pagination: '.pagination-1',
            mode: 'vertical',
            slidesPerView: 1,
            paginationClickable: true,
            speed: 800,
            noSwiping: true
        });
        var jxStage = new JxSwiper(swiper);
    })

</script>
