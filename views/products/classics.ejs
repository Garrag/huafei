                 style="margin: auto;position: absolute; left: 25%; top: 0; bottom: 0;width: auto;height: 150px;display: block">
            <img id="ani" data-ani="animated fadeInRight" src="/images/products/classics/3/text.png"
                 style="margin: auto;position: absolute; right: 10%; top: 0; bottom: 0">
        </section>
        <!--第4页-->
        <section class="swiper-slide swiper-no-swiping" style="background: #FFF;position: relative">
            <img src="/images/products/classics/4/bg.png"
