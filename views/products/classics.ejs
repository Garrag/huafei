                 style="margin: auto;position: absolute; left: 0; right: 0; top: 5%;">
            <img id="ani" data-ani="animated fadeIn" src="/images/products/classics/6/role1.png"
                 style="margin: auto;position: absolute; right:0; left: 0; bottom: 5%;width: auto;height: 70%;display: block">
        </section>
        <!--第7页-->
        <section class="swiper-slide swiper-no-swiping" style="background: #000; position: relative">
            <img id="ani" data-ani="animated slideInUp" src="/images/products/classics/7/role1.png"
                 style="margin: auto;position: absolute; bottom: 0;left: 0;right: 0; width: auto;height:35%;display: block">
            <img id="ani" data-ani="animated fadeInUp" src="/images/products/classics/7/text1.png"
                 style="margin: auto;position: absolute;top:10%; left: 0;right: 0;z-index: 3">
        </section>
        <!--第8页-->
        <section class="swiper-slide swiper-no-swiping" style="background: #000; position: relative">
            <img id="ani" data-ani="animated slideInUp" data-open="1-" src="/images/products/classics/8/role1.png"
                 style="margin: auto;position: absolute; bottom: 0;left: 0;right: 0;top: 0; width: auto;height:35%;display: block">
            <img id="ani" data-ani="animated fadeInDown" src="/images/products/classics/8/text1.png"
                 style="margin: auto;position: absolute;top:10%; left: 0;right: 0;z-index: 3">
            <img id="ani" data-ani="animated slideInUp" data-close="0-" src="/images/products/classics/8/role2.png"
                 style="margin: auto;position: absolute; bottom: 0;top: 0;left: 30%; width: auto;height:35%;display: block">
            <img id="ani" data-ani="animated fadeInLeft" src="/images/products/classics/8/role3.png"
                 style="margin: auto;position: absolute; bottom: 0;right: 30%;top: 0; width: auto;height:35%;display: block">
