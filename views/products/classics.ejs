        </section>
        <!--第5页-->
        <section class="swiper-slide swiper-no-swiping" style="background: #000;position: relative">
            <img id="ani" data-ani="animated fadeIn" src="/images/products/classics/5/role.png"
                 style="margin: auto;position: absolute; right:10%; top: 0; bottom: 0;width: auto;height: 80%;display: block">
            <img id="ani" data-ani="animated fadeInLeft" data-over="true" src="/images/products/classics/5/text1.png"
                 style="margin: auto;position: absolute; left: 10%; top: 0; bottom: 0">
            <img id="ani" data-ani="animated fadeInUp" src="/images/products/classics/5/text2.png"
                 style="margin: auto;position: absolute; left: 10%; top: 0; bottom: 0">
        </section>
        <!--第6页-->
        <section class="swiper-slide swiper-no-swiping" style="background: #000;position: relative">
